{
  "_version": "1.42.0",
  "sap.app": {
    "id": "project3",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.1",
      "toolsId": "d1a40033-ed7a-4cb8-b161-66f0a52632fc"
    },
    "dataSources": {
      "bpService": {
        "uri": "/bp/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "glService": {
        "uri": "/gl/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "cocdService": {
        "uri": "/cocd/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }

    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "componentUsages": {
      "glaccountComponent" : {
        "name" : "project2",
        "settings" : {},
        "componentData" : {},
        "lazy" : true
      },
      "customerComponent" : {
        "name" : "project3",
        "settings" : {},
        "componentData" : {},
        "lazy" : true
      },
      "companyCodeComponent" : {
        "name" : "project4",
        "settings" : {},
        "componentData" : {},
        "lazy" : true
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project3.i18n.i18n"
        }
      },
      "BP" : {
        "dataSource": "bpService",
        "preload": true,
        "settings": {
          "operationMode" : "Server",
          "earlyRequests" : true,
          "synchronizationMode" : "None",
          "autoExpandSelect" : true
        }
      },
      "GL" : {
        "dataSource": "glService",
        "preload": true,
        "settings": {
          "operationMode" : "Server",
          "earlyRequests" : true,
          "synchronizationMode" : "None",
          "autoExpandSelect" : true
        }
      },
      "CoCd" : {
        "dataSource": "cocdService",
        "preload": true,
        "settings": {
          "operationMode" : "Server",
          "earlyRequests" : true,
          "synchronizationMode" : "None",
          "autoExpandSelect" : true
        }
      },
      "SelectOption" : {
        "type" : "sap.ui.model.json.JSONModel",
        "uri" : "model/SelectOption.json"
      },
      "BpCreditStatus" :{
        "type" : "sap.ui.model.json.JSONModel",
        "uri" : "model/BpCreditStatus.json" 
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "project3.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Home",
          "pattern": "",
          "target": [
            "Home"
          ]
        },
        {
          "name": "Customer",
          "pattern": "",
          "target": {
            "name" : "Customer",
            "prefix": "Customer"
          }
        },
        {
          "name": "CustomerDetail",
          "pattern": "CustomerDetail/{num}",
          "target": {
            "name" : "CustomerDetail",
            "prefix": "CustomerDetail"
          }
        },
        {
          "name": "CustomerList",
          "pattern": "CustomerList",
          "target": {
            "name" : "CustomerList",
            "prefix": "CustomerList"
          }
        },
        {
          "name": "CreateCustomer",
          "pattern": "CreateCustomer/{bpCategory}",
          "target": {
            "name" : "CreateCustomer",
            "prefix": "CreateCustomer"
          }
        },
        {
          "name": "CustomerChart",
          "pattern": "CustomerChart",
          "target": {
            "name" : "CustomerChart",
            "prefix": "CustomerChart"
          }
        },
        {
          "name": "CustomerChartDetail",
          "pattern": "CustomerChartDetail/{type}",
          "target": {
            "name" : "CustomerChartDetail",
            "prefix": "CustomerChartDetail"
          }
         },
         {
          "name": "CustomerSubmitChartDetail",
          "pattern": "CustomerSubmitChartDetail/{submitState}",
          "target": {
            "name" : "CustomerSubmitChartDetail",
            "prefix": "CustomerSubmitChartDetail"
          }
        }
      ],
      "targets": {
        "Home" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "Home",
          "viewName": "Home"
        },
        "CustomerDetail": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "CustomerDetail",
          "viewName": "CustomerDetail"
        },
        "CustomerList" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "CustomerList",
          "viewName": "CustomerList"
        },
        "CreateCustomer" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "CreateCustomer",
          "viewName": "CreateCustomer"
        },
        "CustomerChart" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "CustomerChart",
          "viewName": "CustomerChart"
        },
        "CustomerChartDetail" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "CustomerChartDetail",
          "viewName": "CustomerChartDetail"
        },
        "CustomerSubmitChartDetail" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "CustomerSubmitChartDetail",
          "viewName": "CustomerSubmitChartDetail"
        }
      }
    },
    "rootView": {
      "viewName": "project3.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
