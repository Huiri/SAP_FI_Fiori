            <HBox id="_IDGenHBox1" width="100%" height="3rem">
                <Title id="_IDGenTitle1"  text="고객 마스터"/>
                 
            </HBox>
            <GenericTile id="_IDGenGenericTile1" class="sapUiTinyMarginBegin sapUiTinyMarginTop"
                header="고객 마스터 조회"
                press=".toCustomerList">
                <TileContent id="_IDGenTileContent1" >
                    <ImageContent id="_IDGenImageContent1" src="sap-icon://crm-sales"/>
                </TileContent>
            </GenericTile>
           <GenericTile id="_IDGenGenericTile2" class="sapUiTinyMarginBegin sapUiTinyMarginTop"
                header="고객 분석"
               
                press=".toCustomerChart">
                <TileContent id="_IDGenTileContent2" >
                    <ImageContent id="_IDGenImageContent2" src="sap-icon://bar-chart"/>
                </TileContent>
            </GenericTile>
